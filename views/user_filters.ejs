<%= include part_user_head %>
	<div class="row col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1"  style="padding-left:10px; padding-right:10px;">
		
		<div class="col-sm-4 col-xm-12" style="box-sizing: border-box;">
			<% filters.forEach(function(filter, index){ %>
				<% if(index%3 == 0){ %>
					<div class="card">
						<a class="gal" href="/categories/<%= filter.filter_id %>">
							<img class="gal-img" src="<%= filter.image_url %>">
							<h1 class="gal-title"><%= filter.type %></h1>
						</a>
					</div>
				<% } %>
			<% }) %>
		</div>
		<div class="col-sm-4 col-xm-12" style="box-sizing: border-box;">
			<% filters.forEach(function(filter, index){ %>
				<% if(index%3 == 1){ %>
					<div class="card">
						<a class="gal" href="/categories/<%= filter.filter_id %>">
							<img class="gal-img" src="<%= filter.image_url %>">
							<h1 class="gal-title"><%= filter.type %></h1>
						</a>
					</div>
				<% } %>
			<% }) %>
		</div>
		<div class="col-sm-4 col-xm-12" style="box-sizing: border-box;">
			<% filters.forEach(function(filter, index){ %>
				<% if(index%3 == 2){ %>
					<div class="card">
						<a class="gal" href="/categories/<%= filter.filter_id %>">
							<img class="gal-img" src="<%= filter.image_url %>">
							<h1 class="gal-title"><%= filter.type %></h1>
						</a>
					</div>
				<% } %>
			<% }) %>
		</div>
	</div>
<%= include part_foot %>